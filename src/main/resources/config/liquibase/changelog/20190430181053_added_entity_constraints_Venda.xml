        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="venda"
                                 constraintName="fk_venda_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

    </changeSet>
</databaseChangeLog>
